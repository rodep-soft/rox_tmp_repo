[Unit]
Description=ROS2 Robot (Docker Compose)

[Service]
Restart=always
ExecStart=/usr/bin/docker compose -f /home/rodep/env/rox_tmp_repo/automation/docker-compose.yml up
ExecStop=/usr/bin/docker compose -f /home/rodep/env/rox_tmp_repo/automation/docker-compose.yml down

[Install]
WantedBy=multi-user.target
